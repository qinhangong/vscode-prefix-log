{
    "name": "vscode-prefix-log",
    "displayName": "vscode-prefix-log",
    "description": "console with prefix",
    "version": "0.0.8",
    "engines": {
        "vscode": "^1.31.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/qinhangong/vscode-prefix-log"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:console.with.prefix"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "console.with.prefix",
                "title": "prefix.log"
            }
        ],
        "keybindings": [
            {
                "command": "console.with.prefix",
                "key": "shift+1",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.3.1",
        "vscode": "^1.1.28",
        "eslint": "^5.13.0",
        "@types/node": "^10.12.21",
        "@types/mocha": "^2.2.42"
    },
    "publisher": "zhangleilei"
}
